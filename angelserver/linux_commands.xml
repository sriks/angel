<commands player="rhythmbox" command="rhythmbox-client ">

<operation id="nowplaying">
<option>--print-playing</option>
</operation>

<operation id="trackduration">
<option>--print-playing --%td</option>
</operation>

<operation id="play">
<option>--play</option>
</operation>

<operation id="pause">
<option>--pause</option>
</operation>

<operation id="next">
<option>--next</option>
</operation>

<operation id="prev">
<option>--previous</option>
</operation>


<operation id="quit">
<option>--quit</option>
</operation>

</commands>

